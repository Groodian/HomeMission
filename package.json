{
  "name": "homemission",
  "private": true,
  "scripts": {
    "emit-schema": "ts-node src/lib/graphql/schema.ts",
    "codegen": "npm run emit-schema && graphql-let",
    "dev": "next dev",
    "build": "npm run codegen && next build",
    "start": "next start -p 80",
    "type-check": "tsc --incremental --pretty --noEmit --tsBuildInfoFile ./.next/cache/.tsbuildinfo && tsc --project cypress/tsconfig.json",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest --forceExit --detectOpenHandles",
    "test:ci": "jest --forceExit --detectOpenHandles --ci --reporters=default --reporters=jest-junit",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "test-all": "npm run codegen && npm run lint && npm run type-check && npm test && npm run e2e:headless",
    "prepare": "husky install && npm run codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@auth0/nextjs-auth0": "^1.6.2",
    "@devexpress/dx-react-chart": "^2.7.6",
